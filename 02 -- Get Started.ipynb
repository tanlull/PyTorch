   "source": [
    "import torch as T\n",
    "a = T.zeros(3, 5) # zero matrix\n",
    "b = T.ones(3, 5) # one matrix\n",
    "c = T.eye(3, 5) # identity matrix\n",
    "\n",
    "d = a + b + 4 * c\n",
    "print('a + b + 4c =\\n{}'.format(d))"
